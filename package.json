    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/express-sslify": "^1.2.5",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.7",
